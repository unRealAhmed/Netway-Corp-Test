    "eslint-plugin-prettier": "^5.2.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
